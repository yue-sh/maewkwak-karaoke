datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["interactiveTransactions"]
  binaryTargets   = ["native", "rhel-openssl-1.1.x", "linux-arm64-openssl-1.1.x"]
  output          = "../generated/client"
}

// main

model Song {
  id     String @id @default(cuid())
  songId String
  title  String
  artist String

  @@index([title, artist])
  @@map("songs")
}
