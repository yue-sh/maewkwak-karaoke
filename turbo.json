{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"dev": {
			"cache": false
		},
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				".next/**",
				"dist/**"
			]
		},
		"start": {
			"cache": false
		},
		"start:prod": {
			"cache": false
		},
		"db:seed": {
			"cache": false
		},
		"db:generate": {
			"cache": false
		}
	}
}
